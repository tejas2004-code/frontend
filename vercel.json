{
  "version": 2,
  "name": "mern-ecommerce-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_FETCH_PRODUCT": "http://localhost:5000/products",
    "REACT_APP_ADMIN_UPDATE_PRODUCT": "http://localhost:5000/admin/update",
    "REACT_APP_ADMIN_DELETE_PRODUCT": "http://localhost:5000/admin/delete",
    "REACT_APP_GET_USER_DETAILS": "http://localhost:5000/user/details",
    "REACT_APP_UPDATE_USER_DETAILS": "http://localhost:5000/user/update",
    "REACT_APP_RESET_PASSWORD": "http://localhost:5000/reset-password",
    "REACT_APP_DELETE_USER_DETAILS": "http://localhost:5000/user/delete"
  }
 
} 
